{
  "kind": "collectionType",
  "collectionName": "results",
  "info": {
    "name": "Result"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "createdAt",
      "updatedAt"
    ],
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "iteration": {
      "type": "integer",
      "default": 1,
      "min": 1,
      "required": true
    },
    "answers": {
      "type": "component",
      "repeatable": true,
      "component": "items.answer"
    },
    "user": {
      "via": "surveyResults",
      "plugin": "users-permissions",
      "model": "user"
    },
    "survey": {
      "via": "results",
      "model": "survey"
    }
  }
}
